  "name": "Supplier Projection Report",
  "prepared_report": 0,
  "query": "SELECT\n    po.transaction_date AS po_date,\n    CONCAT('<a href=\"/app/purchase-order/', po.name, '\">', po.name, '</a>') AS purchase_order,\n    CONCAT('<a href=\"/app/purchase-order/view/report?docstatus=1&supplier=', po.supplier, '\">', po.supplier, '</a>') AS supplier,\n    GROUP_CONCAT(DISTINCT CONCAT('<a href=\"/app/purchase-invoice/', pi.name, '\">', pi.name, '</a>')) AS purchase_invoices,\n    po.grand_total AS po_amount\nFROM\n    `tabPurchase Invoice` AS pi\nJOIN\n    `tabPurchase Invoice Item` AS pii ON pi.name = pii.parent\nJOIN\n    `tabPurchase Order` AS po ON pii.purchase_order = po.name\nWHERE\n    pi.docstatus = 1\n    AND pi.posting_date BETWEEN %(from_date)s AND %(to_date)s\nGROUP BY\n    po.name\nORDER BY\n    po.transaction_date;\n",
  "ref_doctype": "Supplier",
