  "name": "LPO supply balance report",
  "prepared_report": 0,
  "query": "SELECT\n    po.transaction_date AS po_date,\n    CONCAT('<a href=\"/app/purchase-order/', po.name, '\">', po.name, '</a>') AS purchase_order_id,\n    CONCAT('<a href=\"/app/purchase-order/', po.name, '\">', po.supplier, '</a>') AS supplier,\n    po.status AS po_status,\n\n\n    poi.item_code,\n    poi.item_name, \n    poi.item_group, \n    poi.rate,                             \n    poi.qty AS ordered_qty,               \n    poi.received_qty,                     \n    (poi.qty - poi.received_qty) AS balance_qty\n                 \nFROM\n    `tabPurchase Order Item` AS poi         \nJOIN\n    `tabPurchase Order` AS po ON poi.parent = po.name \nJOIN\n    `tabItem` item ON poi.item_code = item.name \nWHERE\n    po.docstatus = 1 \n    AND po.status NOT IN ('Cancelled', 'Closed') \n    AND (poi.qty - poi.received_qty) > 0\n    AND po.transaction_date BETWEEN %(from_date)s AND %(to_date)s\nGROUP BY\npo.transaction_date,\npo.supplier\n    \nORDER BY\npo.transaction_date,\n    po.supplier,                          \n    item.item_group,\n    poi.item_code;\n",
  "ref_doctype": "Purchase Order",
  "reference_report": null,
  "report_name": "LPO supply balance report",
  "report_script": "frappe.query_reports[\"LPO supply balance report\"] = {\n    onload: function(report) {\n        report.filters.supplier = report.filters.supplier || '';\n        report.filters.item_name = report.filters.item_name || '';\n        report.filters.purchase_order_id = report.filters.purchase_order_id || '';\n    }\n};\n",
