  "module": "Stock",
  "name": "Stock Balance Report",
  "prepared_report": 0,
  "query": "SELECT\n    sle.item_code,\n    item.item_name,\n    item.stock_uom AS uom,\n    sle.warehouse,\n    SUM(CASE WHEN sle.voucher_type = 'Stock Entry' AND sle.actual_qty < 0 THEN ABS(sle.actual_qty) ELSE 0 END) AS issued_qty,\n    (SELECT SUM(actual_qty)\n     FROM `tabBin`\n     WHERE item_code = sle.item_code\n       AND warehouse = sle.warehouse) AS current_balance_qty\n\n\nFROM\n    `tabStock Ledger Entry` sle\nJOIN\n    `tabItem` item ON item.name = sle.item_code\nWHERE\n    sle.docstatus = 1\n    AND sle.posting_date BETWEEN %(from_date)s AND %(to_date)s\n    \nGROUP BY\n    sle.item_code, sle.warehouse\nHAVING \n    current_balance_qty > 0 \nORDER BY\n    sle.item_code, sle.warehouse;\n",
  "ref_doctype": "Stock Ledger Entry",
