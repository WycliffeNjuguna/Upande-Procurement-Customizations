  "name": "Item Price History Report",
  "prepared_report": 0,
  "query": "SELECT\n    pi.posting_date,\n    pii.uom,\n    \n    pii.item_code,\n    pii.item_name,\n    pii.item_group,\n    pi.supplier,\n    pii.rate,\n    pi.currency,\n   CONCAT('<a href=\"/app/purchase-invoice/', pi.name, '\">', pi.name, '</a>') AS purchase_invoice\nFROM\n    `tabPurchase Invoice Item` AS pii\nJOIN\n    `tabPurchase Invoice` AS pi ON pii.parent = pi.name\nWHERE\n    pi.docstatus = 1\n \n    \n    AND pi.posting_date BETWEEN %(from_date)s AND %(to_date)s\nORDER BY\n    pi.posting_date DESC\n",
